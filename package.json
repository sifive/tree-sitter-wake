{
  "name": "tree-sitter-wake",
  "version": "0.18.2",
  "description": "Wake grammar for tree-sitter",
  "license": "Apache-2.0",
  "author": "SiFive",
  "homepage": "https://github.com/sifive/tree-sitter-wake#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sifive/tree-sitter-wake.git"
  },
  "bugs": {
    "url": "https://github.com/sifive/tree-sitter-wake/issues"
  },
  "main": "index.js",
  "files": [
    "grammar.js",
    "binding.gyp",
    "src/*"
  ],
  "keywords": [
    "parser",
    "tree-sitter",
    "wake"
  ],
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "gen": "tree-sitter generate",
    "install": "node bin/install.js",
    "prepare": "node bin/install.js",
    "test": "tree-sitter test",
    "linter": "eslint grammar.js"
  },
  "dependencies": {
    "nan": "^2.13.1"
  },
  "devDependencies": {
    "@drom/eslint-config": "^0.10.0",
    "eslint": "^5.16.0",
    "tree-sitter-cli": "^0.15.14"
  },
  "eslintConfig": {
    "extends": "@drom/eslint-config/eslint4/node8",
    "rules": {
      "camelcase": 0,
      "array-bracket-spacing": 0,
      "no-unused-vars": 0
    },
    "globals": {
      "grammar": "readonly",
      "field": "readonly",
      "seq": "readonly",
      "choice": "readonly",
      "alias": "readonly",
      "repeat": "readonly",
      "repeat1": "readonly",
      "optional": "readonly"
    }
  },
  "tree-sitter": [
    {
      "scope": "source.wake",
      "file-types": [
        "wake"
      ]
    }
  ]
}
